{
  "name": "discode",
  "version": "1.0.0",
  "description": "A Leetcode integration with Discord",
  "scripts": {
    "start": "node dist/bin/app.js",
    "build": "tsc",
    "dev": "nodemon",
    "register": "node dist/bin/register.js",
    "lint": "npx biome lint",
    "lint:fix": "npx biome lint --write",
    "format": "npx biome format",
    "format:fix": "npx biome format --write",
    "check": "npx biome check",
    "check:fix": "npx biome check --write",
    "migration:up": "DBMATE_MIGRATIONS_DIR=./migrations dbmate up",
    "migration:down": "DBMATE_MIGRATIONS_DIR=./migrations dbmate down",
    "migration:new": "DBMATE_MIGRATIONS_DIR=./migrations dbmate new"
  },
  "author": "H-DNA",
  "license": "Apache-2.0",
  "dependencies": {
    "discord-interactions": "^4.3.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "puppeteer": "^24.16.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.20",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.1",
    "dbmate": "^2.28.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
